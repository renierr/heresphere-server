{
  "id": "/",
  "name": "HSServer",
  "short_name": "HSServer",
  "description": "A web application for managing videos.",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#4A90E2",
  "launch_handler":  {
    "client_mode": ["navigate-existing", "auto"]
  },
  "share_target": {
    "action": "/static/share.html",
    "method": "GET",
    "enctype": "application/x-www-form-urlencoded",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "icons": [
    {
      "src": "/static/icons/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "/static/icons/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "screenshots": [
    {
      "src": "/static/images/screenshot1.png",
      "sizes": "640x480",
      "type": "image/png",
      "form_factor": "wide"
    },
    {
      "src": "/static/images/screenshot2.png",
      "sizes": "320x480",
      "type": "image/png"
    }
  ]

}